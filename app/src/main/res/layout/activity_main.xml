<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tvPerson"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="65dp"
        android:layout_marginEnd="57dp"
        android:text="@string/doctor"
        app:layout_constraintBaseline_toBaselineOf="@+id/tvPatients"
        app:layout_constraintEnd_toStartOf="@+id/tvPatients"
        app:layout_constraintStart_toStartOf="parent"
        tools:lines="2" />

    <TextView
        android:id="@+id/tvPatients"
        android:layout_width="235dp"
        android:layout_height="0dp"
        android:layout_marginTop="88dp"
        android:layout_marginEnd="39dp"
        android:layout_marginBottom="158dp"
        android:lines="15"
        android:text="@string/doctor"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvPerson"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tvDoctor"
        android:layout_width="298dp"
        android:layout_height="26dp"
        android:layout_marginStart="84dp"
        android:layout_marginTop="20dp"
        android:text="@string/doctor"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvPatients" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="291dp"
        android:layout_height="0dp"
        android:layout_marginEnd="28dp"
        android:layout_marginBottom="23dp"
        android:text="@string/queue"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvPatients" />

</android.support.constraint.ConstraintLayout>